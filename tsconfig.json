{
   "compilerOptions": {
      "target": "esnext",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "baseUrl": ".",
      "paths": {
         "@components": ["./components"],
         "@components/*": ["./components/*"],
         "@animations": ["./animations"],
         "@animations/*": ["./animations/*"],
         "@layouts": ["./layouts"],
         "@layouts/*": ["./layouts/*"],
         "@hooks": ["./hooks"],
         "@hooks/*": ["./hooks/*"],
         "@types": ["./types"],
         "@theme": ["./themes"],
         "@utils": ["./utils"],
         "@queries": ["./redux/queries"],
         "@queries/*": ["./redux/queries/*"],
         "@constants": ["./constants"],
         "@constants/*": ["./constants/*"],
         "@hoc": ["./hoc"],
         "@hoc/*": ["./hoc/*"],
         "@partials": ["./partials"],
         "@partials/*": ["./partials/*"],
         "@modals": ["./modals"],
         "@modals/*": ["./modals/*"]
      }
   },
   "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx",
      "pages/portals/admin/subscribers"
, "pages/portals/admin/rto/[id]/student-list.tss"   ],
   "exclude": ["node_modules"]
}
